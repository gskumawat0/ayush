    <section id="about" class="section my-3">
        <h1 class='text-center border-top-bottom py-4'>About me</h1>
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="img-thumbnail">
                        <img src="/images/guitar-1354025_1920.jpg" class='img-responsiverounded-bottom w-100 p-2'></img>
                    </div>
                </div>
                <div class="col-md-6">
                    <article>
