    <section id="contact" class="section my-3">
        <h1 class='text-center border-top-bottom py-4'>Contact us</h1>
        <div class="container">
            <div class="row">
                <div class="row">
                    <div class="col-md-12 w-50">
                        <form>
                            <div class="form-group">
                                <label for="email">Email : </label>
                                <input type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="Enter your email">
                            </div>
                            <div class="form-group">
                                <label for="name">Name : </label>
                                <input type="text" class="form-control" id="name" placeholder="your full name">
                            </div>
                            <div class="form-group">
                                <label for="location">Location :</label>
                                <input type="text" class="form-control" id="location" placeholder="location of concert">
                            </div>
                            <div class="form-group">
                                <label for="date">Date :</label>
                                <input type="date" class="form-control" id="date" placeholder="date of concert">
                            </div>
                            <div class="form-group">
                                <label for="duration">Concert Duration : </label>
                                <input type="text" class="form-control" id="duration" placeholder="duration of concert">
                            </div>
                            <div class="form-group">
                                <label for="message">Message : </label>
                                <textarea name="message" id='message' rows='7'></textarea>
                            </div>

                            <button type="submit" class="btn btn-primary">Submit</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>




